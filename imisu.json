{
  "exposeFullApi": true,
  "serverPort": 8080,
  "healthChecks": {
    "httpHealthChecks": [
      {
        "name": "exampleHttp",
        "target": "https://example.org",
        "enabled": true
      }
    ],
    "dnsHealthChecks": [
      {
        "name": "exampleDns",
        "target": "1.1.1.1",
        "enabled": true
      }
    ],
    "pingHealthChecks": [
      {
        "name": "examplePing",
        "target": "1.1.1.1",
        "enabled": true
      }
    ]
  }
}